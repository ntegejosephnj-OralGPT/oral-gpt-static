{
  "routes": [
    {
      "route": "/.auth/login/aad",
      "statusCode": 200
    },
    {
      "route": "/.auth/logout",
      "statusCode": 200
    },
    {
      "route": "/.auth/me",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif}",
      "/css/*"
    ]
  },
  "responseOverrides": {
    "401": {
      "rewrite": "/index.html"
    },
    "404": {
      "rewrite": "/index.html"
    }
  },
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "clientIdSettingName": "AZURE_CLIENT_ID",
          "clientSecretSettingName": "AZURE_CLIENT_SECRET",
          "tenantIdSettingName": "AZURE_TENANT_ID"
        }
      }
    }
  }
}
