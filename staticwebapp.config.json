{
  "routes": [
    {
      "route": "/.auth/login/aad",
      "statusCode": 200
    },
    {
      "route": "/.auth/logout",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/.auth/*"]
  },
  "responseOverrides": {
    "401": {
      "rewrite": "/index.html"
    },
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  }
}
